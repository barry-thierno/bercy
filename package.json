{
  "name": "react-starter-dojo-react-axa",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@axa-fr/react-toolkit-all": "^1.3.21",
    "@axa-fr/react-toolkit-core": "^1.3.21",
    "create-react-context": "0.3.0",
    "msw": "^0.24.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "sass": "^1.30.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!**/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "rootDir": "./",
  "proxy": "https://bercywebapi.azurewebsites.net/api/v1"
}
